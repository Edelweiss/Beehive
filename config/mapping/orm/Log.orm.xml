<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                   https://raw.github.com/doctrine/doctrine2/master/doctrine-mapping.xsd">
    <entity name="App\Entity\Log" table="log" repository-class="App\Repository\LogRepository">
        <id name="id" type="integer">
            <generator strategy="AUTO"/>
        </id>
        <field name="action" type="string" length="8"/>
        <field name="loggedAt" type="datetime"/>
        <field name="objectId" type="string" length="32"/>
        <field name="objectClass" type="string" length="255"/>
        <field name="version" type="integer"/>
        <field name="data" type="array"/>
        <field name="username" type="string" length="255"/>
    </entity>
</doctrine-mapping>
{% extends 'frontend.html.twig' %}
{% set c = corrections|first %}
{% block beta %}
  {% if c is not null and c is not empty %}
    {{ c.compilation.title }}
  {% endif %}
{% endblock %}

{% block body %}

  {% if c is not null and c is not empty and c.compilation.collection == 'BOEP' %}
    {% include 'apiary/snippetBoep.html.twig' with {'corrections': corrections} %}
  {% else %}
    {% include 'apiary/snippetHoney.html.twig' with {'corrections': corrections} %}
  {% endif %}

{% endblock %}

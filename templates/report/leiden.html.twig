{% extends 'base.html.twig' %}

{% block body %}

  <h3>Einträge für Berichtigungsliste {{ compilation.title }}</h3>
  {% include 'PapyrillioBeehiveBundle:Report:leidenData.html.twig' with {'corrections': corrections} %}
  <p class="actionHolder"><span id="print">Drucken</span></p>
{% endblock %}

{% block javascripts %}
  {{ parent() }}
  
  <script src="{{ asset('js/jquery/jquery-jeditable.min.js') }}" type="text/javascript"></script>

  <script type="text/javascript">
    $(function(){
      $('span#print').click(function(event){window.print();});
    });
  </script>

{% endblock %}